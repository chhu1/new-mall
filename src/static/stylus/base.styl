@charset 'UTF-8';
pxRem(px, base-font-size = 75px)
    if unit(px) == 'rem'
        return px
    return unit(px / base-font-size, 'rem')
    
font24 = pxRem(24)
font28 = pxRem(28)
font32 = pxRem(32)
colBorDeep = #dddddd
colBorLight = #dfdfdf
colBorWhite = #ffffff
colFontWhite = #ffffff
colFontBlack = #333333
colFontGrey = #999999
colRed = #c728c3
